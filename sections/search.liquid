<section>
    {% if search.performed %}
        <h1>Search Page</h1>
        <div>
            {% for result in search.results %}
                <p>{{ result.object_type }}</p>
            {% endfor %}
        </div>
    {%else%}
        <div>
            <h1>Not results found</h1>
        </div>
    {% endif %}
</section>



{% schema %}
    {
        "name": "search",
        "settings": [
            {
                "type": "color",
                "id": "background",
                "label": "Background",
                "default": "#F4F4F4"
            },
            {
                "type": "color",
                "id": "text_color",
                "label": "Text",
                "default": "#000"
            }
        ],
        "presets": [
            {
                "name": "search"
            }
        ]
    }
    {% endschema %}